<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             xmlns:TwiApp="clr-namespace:TwiApp.Views.Alphabet.E2"
             x:Class="TwiApp.Views.Alphabet.E2.LetterE2ContentPage"
             NavigationPage.HasNavigationBar="False"
             x:Name="ThisPage"
             BackgroundColor="#FAFBFF">

    <StackLayout Orientation="Horizontal">
        <TwiApp:LetterE2ContentView
            BindingContext="{Binding SelectedContent}"
            x:Name="ContentView" 
            PreviousClicked="ContentItemView_PreviousClicked"
            NextClicked="ContentItemView_NextClicked" />

    </StackLayout>
</ContentPage>